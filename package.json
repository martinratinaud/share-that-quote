{
  "name": "share-quotes",
  "version": "1.0.0",
  "description": "",
  "main": "webpack.config.js",
  "config": {
    "version": "1.0.4"
  },
  "scripts": {
    "build-content-script": "NODE_ENV=production webpack --config webpack/config.content_script.js",
    "build-event": "cp src/app/event.js build/event.js",
    "build-popup": "NODE_ENV=production webpack --config webpack/config.popup.js",
    "build": "npm run build-content-script && npm run build-popup && npm run build-event && npm run alert-end",
    "dev": "open http://localhost:8080/popup.html && NODE_ENV=development webpack-dev-server --devtool eval --progress --colors --hot --inline --content-base build",
    "alert-end": "terminal-notifier -message 'Build is finished' >/dev/null 2>&1 || echo 'Build is finished'",
    "test": "karma start",
    "release": "cd build;zip -r ../releases/$npm_package_config_version.zip *"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cheerio": "^0.20.0",
    "font-awesome": "^4.5.0",
    "immutable": "^3.7.6",
    "json-loader": "^0.5.4",
    "lodash": "^4.6.1",
    "material-ui": "^0.15.0-alpha.2",
    "mocha": "^2.4.5",
    "qs": "^6.1.0",
    "quote-parser": "^0.1.5",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-fontawesome": "^0.3.3",
    "react-hot-loader": "^1.3.0",
    "react-tap-event-plugin": "^0.2.2",
    "redux": "^3.3.1",
    "webpack": "^1.12.14"
  },
  "devDependencies": {
    "babel-core": "^6.6.5",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "chai": "^3.5.0",
    "css-loader": "^0.23.1",
    "eslint": "^2.4.0",
    "eslint-config-airbnb": "^6.1.0",
    "eslint-plugin-react": "^4.2.2",
    "file-loader": "^0.8.5",
    "karma": "^0.13.22",
    "karma-chai": "^0.1.0",
    "karma-chai-plugins": "^0.7.0",
    "karma-chai-sinon": "^0.1.5",
    "karma-chrome-launcher": "^0.2.3",
    "karma-mocha": "^0.2.2",
    "karma-mocha-reporter": "^2.0.0",
    "karma-phantomjs-launcher": "^0.2.1",
